<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="esca_orm.Sensor" table="Sensor" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="identity">
			</generator>
		</id>
		<property name="cost" column="cost" type="double" not-null="false" lazy="false"/>
		<property name="name" column="name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="type" column="type" type="string" length="50" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
